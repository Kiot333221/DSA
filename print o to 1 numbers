#include <stdio.h>

int main() {
    float start = 0.0f;
    float end = 1.0f;
    float step = 0.1f; 
    int i;

    printf("Floating point numbers between %.1f and %.1f:\n", start, end);

    for (float i = start; i <= end; i += step) {
        printf("%.1f\n", i);
    }
    if (i != end) {
        printf("%.1f\n", end);
    }

    return 0;
}
